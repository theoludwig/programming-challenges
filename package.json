{
  "name": "programming-challenges",
  "version": "1.0.0",
  "description": "programming-challenges brings programming exercises and challenges to improve your algorithmic logic.",
  "private": true,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theoludwig/programming-challenges"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "start": "node --enable-source-maps build/index.js",
    "build": "rimraf ./build && swc ./cli --out-dir ./build",
    "build:dev": "swc ./src --out-dir ./build --watch",
    "build:typescript": "tsc",
    "lint:commit": "commitlint",
    "lint:editorconfig": "editorconfig-checker",
    "lint:markdown": "markdownlint-cli2",
    "lint:eslint": "eslint ./cli --max-warnings 0 --report-unused-disable-directives --ignore-path .gitignore",
    "test": "cross-env NODE_ENV=test node --enable-source-maps --test build/"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "clipanion": "3.2.1",
    "date-and-time": "3.0.2",
    "execa": "8.0.1",
    "log-symbols": "5.1.0",
    "ora": "7.0.1",
    "replace-in-file": "7.0.1",
    "table": "6.8.1",
    "typanion": "3.14.0",
    "validate-npm-package-name": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.78",
    "@tsconfig/strictest": "2.0.1",
    "@types/date-and-time": "0.13.0",
    "@types/mock-fs": "4.13.1",
    "@types/ms": "0.7.31",
    "@types/node": "20.5.1",
    "@types/sinon": "10.0.16",
    "@types/validate-npm-package-name": "4.0.0",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "cross-env": "7.0.3",
    "editorconfig-checker": "5.1.1",
    "eslint": "8.47.0",
    "eslint-config-conventions": "11.0.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "48.0.1",
    "get-stream": "8.0.1",
    "markdownlint-cli2": "0.8.1",
    "markdownlint-rule-relative-links": "2.1.0",
    "mock-fs": "5.2.0",
    "ms": "2.1.3",
    "rimraf": "5.0.1",
    "sinon": "15.2.0",
    "typescript": "5.1.6"
  }
}
